<form class="form-horizontal" {{ form_start(form) }}>
<div class="col-xs-12">
          <div class="box">
             <div class="box-header">
             </div>
             <div class="box-body">
             
              <table id="example2" class="table table-bordered table-hover">
                <tbody>
                    <tr>
                    <div class="form-group">
                        <td>{{form_label(form.nom)}}</td>
                        <td> <div class="col-sm-10">
                    {{form_widget(form.nom)}}
                  </div></td>
                    </tr>
                    <tr>
                    <div class="form-group">
                        <td>{{form_label(form.prenom)}}</td>
                        <td>
                            <div class="col-sm-10">
                                {{form_widget(form.prenom)}}
                            </div>
                        </td>
                    </tr>
                    <tr>
                    <tr>
                    <div class="form-group">
                        <td>{{form_label(form.cin)}}</td>
                        <td> <div class="col-sm-10">
                    {{form_widget(form.cin)}}
                  </div></td>
                    </tr>
                    <div class="form-group">
                        <td>{{form_label(form.email)}}</td>

                        <td> <div class="col-sm-10">
                    {{form_widget(form.email)}}
                  </div></td>
                    </tr>
                    <tr>
                    <div class="form-group">
                        <td>{{form_label(form.numTel)}}</td>
                        <td> <div class="col-sm-10">
                    {{form_widget(form.numTel)}}
                  </div></td>
                    </tr>
                        <tr>
                            <div class="form-group">
                                <td>{{form_label(form.salary)}}</td>
                                <td> <div class="col-sm-10">
                                        {{form_widget(form.salary)}}
                                    </div></td>
                        </tr>
                    <tr>
                    <div class="form-group">
                        <td>{{form_label(form.password)}}</td>
                        <td> <div class="col-sm-10">
                    {{form_widget(form.password)}}
                  </div></td>
                    </tr>
                    
        
           
       
        </tbody>
        </table>
        <div class="box-footer">
                <button type="submit" class="btn btn-info pull-right">{{ button_label|default('Save') }}</button>
              </div>
        </div>
        </div>
        </div>
        
              
               
                


    
    </form {{ form_end(form) }}>


